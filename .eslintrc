{
    "plugins": [
        "prettier",
        "import",
        "@typescript-eslint"
    ],
    "parser": "@typescript-eslint/parser",
    "extends": [
        "plugin:prettier/recommended",
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "endOfLine": 0,
        "no-debugger": "off",
        "no-console": 0,
        "class-methods-use-this": "off",
        "prettier/prettier": "error",
        "@typescript-eslint/no-explicit-any": 2
    }
}

